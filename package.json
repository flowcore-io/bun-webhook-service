{
  "name": "bun-webhook-service",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "test": "bun --env-file=.env.test test",
    "test:watch": "bun --env-file=.env.test test --watch",
    "test:coverage": "bun --env-file=.env.test test --coverage",
    "test:services:up": "bun run --env-file=.env.test test/fixtures/services.fixture.ts action:up",
    "test:services:down": "bun run --env-file=.env.test test/fixtures/services.fixture.ts action:down",
    "lint": "biome lint",
    "format": "biome format",
    "typecheck": "tsc --noEmit",
    "services:up": "docker compose -f docker-compose.dev.yml up -d",
    "services:down": "docker compose -f docker-compose.dev.yml down",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "latest",
    "@types/ioredis": "^5.0.0",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8",
    "fetch-mock": "^12.6.0",
    "ioredis": "^5.9.0",
    "nats": "^2.29.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@date-fns/utc": "^2.1.1",
    "@flowcore/hono-api": "^0.3.0",
    "@flowcore/pathways": "^0.16.4",
    "@flowcore/sdk": "^1.64.3",
    "@flowcore/sdk-oidc-client": "^1.3.1",
    "@flowcore/time-uuid": "^0.2.0",
    "@hono/zod-openapi": "^1.2.0",
    "bun-sqlite-key-value": "^1.13.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "short-uuid": "^6.0.3",
    "zod": "^3.25.63"
  }
}